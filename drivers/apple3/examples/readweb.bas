10 INVOKE "REQUEST.INV"20 net$=".NETWORK"30 url$="N:HTTPS://www.gnu.org/licenses/gpl-3.0.txt"70 DIM readbuf%(128):array$="readbuf%"80 OPEN #1,net$100 PRINT "READ WEB"105 REM Open110 cnum=ASC("O")115 mode%=4:REM Read Only ( eg http get)120 trans%=3125 ccmd$=CHR$(mode%)+CHR$(trans%)+url$+CHR$(0)+CHR$(LEN(url$))130 clist$=CHR$(LEN(ccmd$)-1)+ccmd$140 PERFORM CONTROL(%cnum,@clist$)net$200 REM Read230 INPUT #1;result$250 PRINT result$;260 IF LEN(result$)>240THEN GOTO 200270 PRINT280 PRINT "All done!!"500 CLOSE #1550 END