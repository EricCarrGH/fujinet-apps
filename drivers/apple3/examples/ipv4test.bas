10 INVOKE "REQUEST.INV"20 net$=".NETWORK"30 url$="N:HTTP://ipv4.icanhazip.com/"50 PRINT "IPV4 TEST"100 OPEN #1,net$ : REM Open network device120 REM FujiNet Open network connnection130 cnum=ASC("O")140 mode%=4:REM Read Only ( eg http get)150 trans%=0:REM No translation160 ccmd$=CHR$(mode%)+CHR$(trans%)+url$+CHR$(0)+CHR$(LEN(url$))170 clist$=CHR$(LEN(ccmd$)-1)+ccmd$180 PERFORM CONTROL(%cnum,@clist$)net$ : REM Control command to FujiNet200 REM Read result from http get220 INPUT #1;result$ : REM from network device250 PRINT "EXTERNAL IP: "+result$500 CLOSE #1550 END